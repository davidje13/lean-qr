{
  "name": "lean-qr",
  "version": "1.2.0",
  "description": "minimal QR code generation",
  "files": [
    "build/**/*",
    "bin/**/*",
    "index.d.ts"
  ],
  "bin": "./bin/cli.js",
  "main": "build/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint --format codeframe --ext mjs,jsx,js src bin",
    "start": "http-server . -d false -c-1 -a localhost -p 8080",
    "test": "lean-test && lean-test --browser=chrome,firefox -x '**/node_modules' -x '**/.*' -x '**/*.int.*'",
    "posttest": "npm run lint",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/lean-qr.git"
  },
  "keywords": [
    "qr",
    "qr-code",
    "barcode"
  ],
  "author": "David Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidje13/lean-qr/issues"
  },
  "homepage": "https://github.com/davidje13/lean-qr#readme",
  "devDependencies": {
    "@neutrinojs/airbnb-base": "9.x",
    "@neutrinojs/library": "9.x",
    "eslint": "7.x",
    "http-server": "^0.12.3",
    "lean-test": "^1.3.1",
    "neutrino": "9.x",
    "pngjs": "^6.0.0",
    "webpack": "4.x",
    "webpack-cli": "3.x"
  }
}
