{
  "name": "lean-qr",
  "version": "2.7.0",
  "description": "minimal QR code generation",
  "private": true,
  "bin": "./cli.mjs",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./nano": {
      "require": "./nano.mjs",
      "import": "./nano.mjs",
      "types": "./index.d.ts"
    },
    "./extras/svg": {
      "require": "./extras/svg.js",
      "import": "./extras/svg.mjs",
      "types": "./index.d.ts"
    },
    "./extras/node_export": {
      "require": "./extras/node_export.js",
      "import": "./extras/node_export.mjs",
      "types": "./index.d.ts"
    },
    "./extras/react": {
      "require": "./extras/react.js",
      "import": "./extras/react.mjs",
      "types": "./index.d.ts"
    },
    "./extras/vue": {
      "require": "./extras/vue.js",
      "import": "./extras/vue.mjs",
      "types": "./index.d.ts"
    },
    "./webcomponent": {
      "require": "./webcomponent.mjs",
      "import": "./webcomponent.mjs",
      "types": "./webcomponent.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "start": "./tools/build.sh && python3 -m http.server -b 127.0.0.1 -d web 8080",
    "test": "lean-test --target=node,chrome,firefox -x '**/*.node.*' -x '**/*.browser.*' && lean-test -i '**/*.node.test.*' && TEST=true lean-test --target=chrome,firefox --preprocess=rollup -i '**/*.browser.test.*' && TEST=true NO_MINIFY=true lean-test --target=chrome,firefox --preprocess=rollup -i '**/*.browser.raw.test.*' && ./test-package/run.sh && prettier --check .",
    "dopublish": "./tools/build.sh && npm publish package.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/lean-qr.git"
  },
  "keywords": [
    "qr",
    "qr-code",
    "qrcode",
    "barcode",
    "react",
    "react-component",
    "preact",
    "preact-component",
    "vue",
    "vue-component",
    "web-component",
    "console",
    "terminal"
  ],
  "author": "David Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidje13/lean-qr/issues"
  },
  "homepage": "https://github.com/davidje13/lean-qr#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "16.x",
    "@rollup/plugin-terser": "0.4.x",
    "@vue/runtime-dom": "3.x",
    "lean-test": "2.x",
    "pngjs": "7.x",
    "preact": "10.x",
    "prettier": "3.6.2",
    "rollup": "4.x"
  }
}
