{
  "name": "lean-qr",
  "version": "1.3.0",
  "description": "minimal QR code generation",
  "files": [
    "build/**/*",
    "bin/**/*",
    "index.d.ts"
  ],
  "bin": "./bin/cli.js",
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./extras/*": "./build/extras/*.js",
    "./extras/*.js": "./build/extras/*.js",
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "format": "prettier --write .",
    "start": "static-server --index index.html --port 8080",
    "test": "lean-test && lean-test --browser=chrome,firefox -x '**/node_modules' -x '**/.*' -x '**/*.int.*' && ./test-package/run.sh && prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/lean-qr.git"
  },
  "keywords": [
    "qr",
    "qr-code",
    "barcode"
  ],
  "author": "David Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidje13/lean-qr/issues"
  },
  "homepage": "https://github.com/davidje13/lean-qr#readme",
  "devDependencies": {
    "@babel/preset-env": "7.x",
    "@rollup/plugin-babel": "5.x",
    "lean-test": "1.x",
    "pngjs": "6.x",
    "prettier": "2.6.1",
    "rollup": "2.x",
    "rollup-plugin-terser": "7.x",
    "static-server": "2.x"
  }
}
